// Custom combos for Miryoku

// Enter + Tab = Escape combo
// Works on all layers
// Positions 32 (Tab) + 33 (Enter) = Escape

/ {
    combos {
        // Note: compatible property already set by miryoku_kludge_thumbcombos.dtsi
        // Just add our custom combo definitions here

        combo_enter_tab_escape {
            timeout-ms = <50>;
            key-positions = <32 33>;
            bindings = <&kp ESC>;
        };
    };
};
